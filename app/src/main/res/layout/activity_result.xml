<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ResultActivity">

    <TextView
            android:text="@string/textview"
            android:layout_width="226dp"
            android:layout_height="46dp" tools:layout_editor_absoluteY="125dp"
            tools:layout_editor_absoluteX="102dp" android:id="@+id/textView2" tools:text="결과"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" tools:ignore="MissingConstraints"/>
    <ImageView
            android:layout_width="121dp"
            android:layout_height="124dp" app:srcCompat="@drawable/ic_mood_black_24dp"
            tools:layout_editor_absoluteY="292dp" tools:layout_editor_absoluteX="145dp" android:id="@+id/imageView"
            android:tint="@color/colorPrimary" tools:srcCompat="@drawable/ic_mood_black_24dp"
            tools:ignore="MissingConstraints,VectorDrawableCompat" android:contentDescription="@string/todo"/>
</android.support.constraint.ConstraintLayout>